end structure

structure SED_dir_systempreparation_SED/top/SED_anion1_name_SED.pdb 
  number SED_anion1_num_SED
  inside box 0.0 0.0 0.0 SED_box_SED SED_box_SED SED_box_SED
end structure

structure SED_dir_systempreparation_SED/top/SED_anion2_name_SED.pdb 
  number SED_anion2_num_SED
  inside box 0.0 0.0 0.0 SED_box_SED SED_box_SED SED_box_SED
end structure
